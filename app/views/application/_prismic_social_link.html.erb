
<a href="<%= social_link.fragments['url'].url %>" target="_blank" rel="noopener noreferrer">
  <% social_link.fragments['image'].main.tap do |image| %>
    <img src="<%= image.url %>" height="<%= image.height * (size_multiplier || 1) %>" width="<%= image.width * (size_multiplier || 1) %>" alt="<%= 'Link to ' + social_link.fragments['name'].value %>" />
  <% end %>
</a>
